#ifndef CONTRIBUYENTE_H_
#define CONTRIBUYENTE_H_
#define MAX_CHAR 30

typedef struct
{
	int idContribuyente;
	char nombre[MAX_CHAR];
	char apellido[MAX_CHAR];
	char cuil[MAX_CHAR];
	int isEmpty;
} eContribuyente;

void initArray(eContribuyente array[], int size);
int arrayEmpty(eContribuyente array[], int size);

#endif /* CONTRIBUYENTE_H_ */
